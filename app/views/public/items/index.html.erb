  <h2>商品一覧</h2>
    <div>
    <% @items.each do |item| %>
        <div>
          <%= link_to item_path(item.id) do%>
            <%= attachment_image_tag(item, :image, size: "120x120") %>

              <p><%= item.name %></p>
              <p><%= (item.price * 1.08).round %></p>
          <% end %>
        </div>
      <% end %>
    </div>