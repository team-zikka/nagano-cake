<h3>会員一覧</h3>
<table class="table">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.cutomer_id %></td>
      <td>
        <%= link_to admin_customer_path(customer.id) do %>
        <%= customer.full_name %>
        <% end %>
      </td>
      <td><%= customer.email %></td>
      <td><%= customer.is_daleted %></td>
    </tr>
    <% end %>
  </tbody>
</table>